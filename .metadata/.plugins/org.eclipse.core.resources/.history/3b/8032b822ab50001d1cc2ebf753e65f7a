package avaliacaodegoleiros;

public class Chute {
	private int id;
	private int forca;
	private int quadrante;
	private int X;
	private int Y;
	
	public Chute(int id) {
		this.setId(id);
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getForca() {
		return forca;
	}

	public void setForca(int forca) {
		this.forca = forca;
	}

	public int getQuadrante() {
		return quadrante;
	}

	public void setQuadrante(int quadrante) {
		this.quadrante = quadrante;
	}

	public int[] getCoordenadas() {
		int[] coordenadas = {X, Y} ;
		return coordenadas;
	}

	public void setXY(int x, int y) {
		X = x;
		Y = y;
	}
}
